<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentNumbers">

    <fragment
        android:id="@+id/fragmentNumbers"
        android:name="com.example.calculator.FragmentNumbers"
        android:label="fragment_numbers"
        tools:layout="@layout/fragment_numbers" >
        <action
            android:id="@+id/action_fragmentNumbers_to_fragmentCalculate"
            app:destination="@id/fragmentCalculate" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCalculate"
        android:name="com.example.calculator.FragmentCalculate"
        android:label="fragment_calculate"
        tools:layout="@layout/fragment_calculate" />
</navigation>